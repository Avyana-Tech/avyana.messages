#include "std_msgs/msg/Header.idl"

module avyana_map_messages {
  module msg {
    @verbatim(language = "comment", text =
      "Map primitive information")
    struct MapPrimitive
    {
      int64 id;

      @verbatim(language = "comment", text =
        "Type of primitive, such as lane, polygon, line.")
      @default(value = "")
      string primitive_type;
    };
  };
};